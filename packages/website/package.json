{
  "name": "taktische-zeichen-website",
  "private": true,
  "version": "0.9.0",
  "scripts": {
    "clean": "shx rm -rf dist",
    "compile": "webpack",
    "generate": "node scripts/generate.js",
    "build": "npm run generate && NODE_ENV=production npm run compile && eleventy",
    "start": "npm run generate && npm run compile && eleventy --serve",
    "format": "prettier --write views src scripts *.js"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "taktische-zeichen-core": "^0.10.0",
    "taktische-zeichen-react": "^0.10.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "babel-loader": "^10.0.0",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
